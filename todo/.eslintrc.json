{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["google"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "quotes": "off",
    "require-jsdoc": "off",
    "prefer-const": "off",
    "eol-last": "off",
    "indent": "off"
  }
}
